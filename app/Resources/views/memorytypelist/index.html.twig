{% extends 'basemanager.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}

{% block body %}
    <h1>Memorytypelists list</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Memorytypeoption</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for memoryTypeList in memoryTypeLists %}
            <tr>
                <td><a href="{{ path('memorytypelist_show', { 'id': memoryTypeList.id }) }}">{{ memoryTypeList.id }}</a></td>
                <td>{{ memoryTypeList.memoryTypeOption }}</td>
                <td>
                    <a href="{{ path('memorytypelist_show', { 'id': memoryTypeList.id }) }}"> <i class="fas fa-eye fa-lg"></i></a>
                    <a href="{{ path('memorytypelist_edit', { 'id': memoryTypeList.id }) }}"> <i class="fas fa-edit fa-lg"></i></a>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('memorytypelist_new') }}"><i class="far fa-plus-square"></i> Create a new memoryTypeList</a>
{% endblock %}

{%  block menu %}
    {{  include('includes/menu.html.twig') }}
{%  endblock %}