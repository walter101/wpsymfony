{% extends 'basemanager.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}

{% block body %}
    <h1>Operatingsystemlists list</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Operatingsystemname</th>
                <th>Image</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for operatingSystemList in operatingSystemLists %}
            <tr>
                <td><a href="{{ path('operatingsystemlist_show', { 'id': operatingSystemList.id }) }}">{{ operatingSystemList.id }}</a></td>
                <td>{{ operatingSystemList.operatingSystemName }}</td>
                <td>{{ operatingSystemList.image }}</td>
                <td>
                    <a href="{{ path('operatingsystemlist_show', { 'id': operatingSystemList.id }) }}"> <i class="fas fa-eye fa-lg"></i></a>
                    <a href="{{ path('operatingsystemlist_edit', { 'id': operatingSystemList.id }) }}"> <i class="fas fa-edit fa-lg"></i></a>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('operatingsystemlist_new') }}"><i class="far fa-plus-square"></i> Create a new operatingSystemList</a>

{% endblock %}

{%  block menu %}
    {{  include('includes/menu.html.twig') }}
{%  endblock %}